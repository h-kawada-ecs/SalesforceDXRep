<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <compactLayouts>
        <fullName>Mobile_Contact_Layout</fullName>
        <fields>Name</fields>
        <fields>Phone</fields>
        <fields>Stage__c</fields>
        <fields>Email</fields>
        <label>Mobile Contact Layout</label>
    </compactLayouts>
    <fields>
        <fullName>Data_Quality_Description__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Data_Quality_Score__c =100,&quot;All Contact Details Captured&quot;, &quot;Missing: &quot;&amp;IF( LEN( Phone ) = 0, &quot;Phone, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp;IF(LEN( Email) = 0, &quot;Email, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp;IF( LEN( MailingStreet ) = 0, &quot;Complete Address, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp;IF( LEN(Title) = 0, &quot;Job Title, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp;IF( ISPICKVAL( Salutation,&quot;&quot;), &quot;Salutation&quot;,&quot;&quot;))</formula>
        <label>Data Quality Description</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Data_Quality_Score__c</fullName>
        <externalId>false</externalId>
        <formula>IF( LEN( Phone) = 0, 0,20) + IF( LEN( Email) = 0, 0,20) + IF( LEN(MailingStreet) = 0, 0,20) + IF( LEN(Title) = 0, 0,20) + IF( ISPICKVAL(Salutation,&quot;&quot;), 0,20)</formula>
        <label>Data Quality Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Languages__c</fullName>
        <externalId>false</externalId>
        <label>Languages</label>
        <length>100</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Level__c</fullName>
        <externalId>false</externalId>
        <label>Level</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Secondary</fullName>
                    <default>false</default>
                    <label>Secondary</label>
                </value>
                <value>
                    <fullName>Tertiary</fullName>
                    <default>false</default>
                    <label>Tertiary</label>
                </value>
                <value>
                    <fullName>Primary</fullName>
                    <default>false</default>
                    <label>Primary</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Location__c</fullName>
        <displayLocationInDecimal>true</displayLocationInDecimal>
        <externalId>false</externalId>
        <label>Location</label>
        <required>false</required>
        <scale>7</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Location</type>
    </fields>
    <fields>
        <fullName>Stage__c</fullName>
        <externalId>false</externalId>
        <label>Stage</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Prospect</fullName>
                    <default>false</default>
                    <label>Prospect</label>
                </value>
                <value>
                    <fullName>Showing</fullName>
                    <default>false</default>
                    <label>Showing</label>
                </value>
                <value>
                    <fullName>Offer</fullName>
                    <default>false</default>
                    <label>Offer</label>
                </value>
                <value>
                    <fullName>Closing</fullName>
                    <default>false</default>
                    <label>Closing</label>
                </value>
                <value>
                    <fullName>Archive</fullName>
                    <default>false</default>
                    <label>Archive</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <listViews>
        <fullName>AllContacts</fullName>
        <filterScope>Everything</filterScope>
        <label>All Contacts</label>
    </listViews>
    <listViews>
        <fullName>BirthdaysThisMonth</fullName>
        <filterScope>Everything</filterScope>
        <filters>
            <field>CONTACT.BIRTHDATE</field>
            <operation>equals</operation>
            <value>THIS_MONTH</value>
        </filters>
        <label>Birthdays This Month</label>
    </listViews>
    <listViews>
        <fullName>MyContacts</fullName>
        <filterScope>Mine</filterScope>
        <label>My Contacts</label>
    </listViews>
    <listViews>
        <fullName>NewLastWeek</fullName>
        <filterScope>Everything</filterScope>
        <filters>
            <field>CONTACT.CREATED_DATE</field>
            <operation>equals</operation>
            <value>LAST_WEEK</value>
        </filters>
        <label>New Last Week</label>
    </listViews>
    <listViews>
        <fullName>NewThisWeek</fullName>
        <filterScope>Everything</filterScope>
        <filters>
            <field>CONTACT.CREATED_DATE</field>
            <operation>equals</operation>
            <value>THIS_WEEK</value>
        </filters>
        <label>New This Week</label>
    </listViews>
    <validationRules>
        <fullName>Contact_must_be_in_Account_ZIP_Code</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
NOT( ISBLANK( AccountId ) ),
MailingPostalCode &lt;&gt;  Account.ShippingPostalCode  
)</errorConditionFormula>
        <errorDisplayField>MailingPostalCode</errorDisplayField>
        <errorMessage>Contact must be in Account ZIP Code</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Google_Info</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <hasMenubar>false</hasMenubar>
        <hasScrollbars>true</hasScrollbars>
        <hasToolbar>false</hasToolbar>
        <height>600</height>
        <isResizable>true</isResizable>
        <linkType>url</linkType>
        <masterLabel>Google Info</masterLabel>
        <openType>newWindow</openType>
        <position>none</position>
        <protected>false</protected>
        <showsLocation>false</showsLocation>
        <showsStatus>false</showsStatus>
        <url>http://google.com?q={!Contact.Name}</url>
    </webLinks>
</CustomObject>
